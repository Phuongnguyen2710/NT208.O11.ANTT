<!-- Thêm thư viện -->
@model PagedList.IPagedList<TicketLand_project.Models.movy>
@using PagedList.Mvc;
<!-- Kết thúc-->


@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout_admin.cshtml";
}

<h1 class="custom_h1"><b>QUẢN LÝ PHIM</b></h1>


<div class="container_custom">
    <p>
        <button class="btn btn-xs btn-success" data-toggle="modal" data-target="#addmovieModal">
            <i class="fa fa-plus custom_i"></i> Tạo phim
        </button>
    </p>

    <div id="roomListContainer">
        @Html.Partial("~/Areas/Admin/Views/movies/Movie_partial.cshtml", Model)
    </div>
</div>

<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.min.js"></script>
<script src="~/Areas/Assets/js/Movie.js"></script>
